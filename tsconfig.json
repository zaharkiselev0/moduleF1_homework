{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "lib": ["DOM", "ES6"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "paths": {
      "@typesLocal/*": ["types/*"],
      "@api/*": ["resources/api/*"],
      "@hooks/*": ["hooks/*"],
      "@components/*": ["components/*"],
      "@resources/*": ["resources/*"]
    }
  },
  "include": ["src"]
}